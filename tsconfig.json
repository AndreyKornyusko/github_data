    "noFallthroughCasesInSwitch": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "**/*.config.js",
    "**/*.ts",
    "**/*.tsx",
    "components/pages/recordVideo/ReactVideoRecorder/components/defaults/record-button.js"
, "src/components/pages/builder/components/navigation/index.tsx", "src/components/pages/builder/components/Builder.tsx", "src/components/pages/builder/index.tsx", "src/components/pages/builder/components/choiceEditor/choiceNextSelect/withClickOutside.js", "src/components/usersSearch/userSearch.js"  ]
}
